  <!-- Guest Name Loading Script -->
  <script>
    class GuestNameHandler {
      constructor() {
        // Embedded guest data - easier to maintain and works with file:// protocol
        this.guestData = new Map([
          ['daniel', 'Hoàng Danh'],
          ['john', 'Nguyễn Văn An'],
          ['mary', 'Trần Thị Bình'],
          ['peter', 'Lê Văn Cường'],
          ['anna', 'Phạm Thị Dung'],
          ['michael', 'Võ Minh Tuấn'],
          ['sarah', 'Nguyễn Thị Lan'],
          ['david', 'Trần Văn Đức'],
          ['lisa', 'Lê Thị Mai'],
          ['robert', 'Phạm Văn Nam']
        ]);
        
        this.isLoaded = true;
        this.init();
      }
      
      async init() {
        try {
          this.updateGuestName();
        } catch (error) {
          console.warn('Guest name handler initialization failed:', error);
        }
      }
      
      // Alternative method: Try to load from external file first, fall back to embedded data
      async loadFromFileWithFallback() {
        try {
          // Only try to fetch if we're not on file:// protocol
          if (window.location.protocol !== 'file:') {
            const response = await fetch('guests.txt');
            
            if (response.ok) {
              const text = await response.text();
              this.parseGuestDataFromFile(text);
              console.log('Guest data loaded from external file');
              return true;
            }
          }
        } catch (error) {
          console.log('External file not available, using embedded data');
        }
        
        console.log('Using embedded guest data');
        return false;
      }
      
      parseGuestDataFromFile(text) {
        this.guestData.clear();
        const lines = text.split('\n');
        
        lines.forEach((line, index) => {
          const trimmedLine = line.trim();
          if (!trimmedLine || trimmedLine.startsWith('#')) {
            return; // Skip empty lines and comments
          }
          
          const equalIndex = trimmedLine.indexOf('=');
          if (equalIndex === -1) {
            console.warn(`Invalid format on line ${index + 1}: ${trimmedLine}`);
            return;
          }
          
          const code = trimmedLine.substring(0, equalIndex).trim();
          const name = trimmedLine.substring(equalIndex + 1).trim();
          
          if (code && name) {
            this.guestData.set(code.toLowerCase(), name);
          }
        });
      }
      
      getGuestCode() {
        const urlParams = new URLSearchParams(window.location.search);
        const guestCode = urlParams.get('g');
        return guestCode ? guestCode.toLowerCase().trim() : null;
      }
      
      getGuestName(code) {
        return this.guestData.get(code) || null;
      }
      
      updateGuestName() {
        const guestCode = this.getGuestCode();
        
        if (!guestCode) {
          console.log('No guest code provided in URL');
          return;
        }
        
        const guestName = this.getGuestName(guestCode);
        
        if (!guestName) {
          console.warn(`Guest not found for code: ${guestCode}`);
          return;
        }
        
        // Wait for DOM to be ready
        if (document.readyState === 'loading') {
          document.addEventListener('DOMContentLoaded', () => this.replaceGuestName(guestName));
        } else {
          this.replaceGuestName(guestName);
        }
      }
      
      replaceGuestName(guestName) {
        const guestElement = document.getElementById('guest-name');
        
        if (guestElement) {
          // Store original content for potential restoration
          if (!guestElement.dataset.originalContent) {
            guestElement.dataset.originalContent = guestElement.innerHTML;
          }
          
          // Replace with guest name while maintaining styling
          guestElement.innerHTML = `<em><strong>${this.escapeHtml(guestName)}</strong></em>`;
          
          console.log(`Guest name updated to: ${guestName}`);
          
          // Add a class for potential CSS styling
          guestElement.classList.add('personalized-guest');
        } else {
          console.error('Guest name element not found');
        }
      }
      
      escapeHtml(text) {
        const div = document.createElement('div');
        div.textContent = text;
        return div.innerHTML;
      }
      
      // Method to add new guests programmatically
      addGuest(code, name) {
        this.guestData.set(code.toLowerCase(), name);
        console.log(`Added guest: ${code} → ${name}`);
      }
      
      // Method to remove guests
      removeGuest(code) {
        const removed = this.guestData.delete(code.toLowerCase());
        if (removed) {
          console.log(`Removed guest: ${code}`);
        }
        return removed;
      }
      
      // Method to get all guests
      getAllGuests() {
        return Array.from(this.guestData.entries()).map(([code, name]) => ({ code, name }));
      }
      
      // Debug method to check current status
      getStatus() {
        return {
          isLoaded: this.isLoaded,
          totalGuests: this.guestData.size,
          currentGuestCode: this.getGuestCode(),
          currentGuestName: this.getGuestCode() ? this.getGuestName(this.getGuestCode()) : null,
          allGuests: this.getAllGuests()
        };
      }
    }
    
    // Initialize the guest name handler
    let guestHandler;
    
    // Initialize when script loads
    (() => {
      guestHandler = new GuestNameHandler();
      
      // Try to load from external file as well (for server environments)
      guestHandler.loadFromFileWithFallback();
      
      // Make it available globally for debugging
      window.guestHandler = guestHandler;
    })();
  </script>
  
